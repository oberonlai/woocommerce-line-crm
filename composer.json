{
    "name": "dailywp/order-chatz",
    "description": "OrderChatz - WooCommerce LINE CRM Plugin",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "DailyWP",
            "email": "m615926@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "firebase/php-jwt": "^6.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "OrderChatz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OrderChatz\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "strauss": {
            "target_directory": "vendor-prefixed",
            "namespace_prefix": "OrderChatz\\Vendor\\",
            "classmap_prefix": "OrderChatz_Vendor_",
            "constant_prefix": "ORDERCHATZ_VENDOR_",
            "delete_vendor_packages": true,
            "delete_vendor_files": true,
            "include_root_autoload": true
        }
    }
}
